            android:text="@string/loan_money_start_end_date_text"
            android:textColor="@color/color_333333"
            android:textSize="@dimen/T4" />

        <TextView
            android:id="@+id/pay_money_start_end_date_tv"
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/common_layout_margin"
            android:layout_weight="1.3"
            android:gravity="center_vertical|right"
            android:text="2017-01-26~2017—06-26"
            android:textColor="@color/color_666666"
            android:textSize="@dimen/T4" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="30dip"
        android:background="@color/color_ffffff"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:weightSum="2" >

        <TextView
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/common_layout_margin"
            android:layout_weight="0.7"
            android:gravity="center_vertical|left"
            android:text="@string/loan_money_day_lilv_text"
            android:textColor="@color/color_333333"
            android:textSize="@dimen/T4" />

        <TextView
            android:id="@+id/pay_money_lilv_day_tv"
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/common_layout_margin"
            android:layout_weight="1.3"
            android:gravity="center_vertical|right"
            android:text="0.01%"
            android:textColor="@color/color_666666"
            android:textSize="@dimen/T4" />
    </LinearLayout>

   
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="30dip"
        android:background="@color/color_ffffff"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:weightSum="2" 
