            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/common_layout_margin"
            android:layout_weight="0.7"
            android:gravity="center_vertical|left"
            android:text="@string/pay_money_loan_state_text"
            android:textColor="@color/color_333333"
            android:textSize="@dimen/T4" />

        <TextView
            android:id="@+id/pay_money_loan_state_tv"
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/common_layout_margin"
