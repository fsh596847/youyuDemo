            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/common_layout_margin"
            android:layout_weight="1.3"
            android:gravity="center_vertical|right"
            android:text="24"
            android:textColor="@color/color_666666"
            android:textSize="@dimen/T4" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="30dip"
        android:background="@color/color_ffffff"
        android:orientation="horizontal"
        android:weightSum="2" 
       >

        <TextView
            android:layout_width="0dip"
            android:layout_height="30dip"
            android:layout_marginLeft="@dimen/common_layout_margin"
            android:layout_weight="0.7"
            android:gravity="center_vertical|left"
            android:text="@string/current_pay_money_date_text"
            android:textColor="@color/color_333333"
            android:textSize="@dimen/T4" />

        <TextView
            android:id="@+id/pay_money_current_pay_date_tv"
            android:layout_width="0dip"
            android:layout_height="30dip"
            android:layout_marginRight="@dimen/common_layout_margin"
            android:layout_weight="1.3"
            android:gravity="center_vertical|right"
            android:text="2017-07-01-26"
            android:textColor="@color/color_666666"
            android:textSize="@dimen/T4" />
    </LinearLayout>
     <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="60dip"
        android:background="@color/color_ffffff"
        android:orientation="horizontal"
        android:weightSum="2" 
       >

        <TextView
            android:layout_width="0dip"
            android:layout_height="30dip"
            android:layout_marginLeft="@dimen/common_layout_margin"
            android:layout_weight="0.5"
            android:gravity="center_vertical|left"
            android:text="@string/loan_money_hetong_text"
            android:textColor="@color/color_333333"
            android:textSize="@dimen/T4" />

        <TextView
            android:id="@+id/pay_money_look_hetong_tv"
            android:layout_width="0dip"
            android:layout_height="30dip"
            android:layout_marginRight="@dimen/common_layout_margin"
            android:layout_weight="1.5"
            android:gravity="center_vertical|right"
            android:text="@string/loan_money_look_hetong_text"
            android:textColor="@color/color_738cc4"
            android:textSize="@dimen/T4" />
    </LinearLayout>

   
