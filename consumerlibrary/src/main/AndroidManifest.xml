<manifest xmlns:android="http://schemas.android.com/apk/res/android"

          package="consumer.fin.rskj.com.consumerlibrary"
    >

    <!-- 添加读写权限 -->
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <!-- 访问互联网权限 -->
    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE" />

    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />
    <!-- 加入读写联系人信息的权限 调用系统通讯录权限-->
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.WRITE_CONTACTS" />

    <!-- 百度地图所需权限-->
    <!-- 这个权限用于进行网络定位-->
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"></uses-permission>
    <!-- 这个权限用于访问GPS定位-->
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"></uses-permission>
    <!-- 用于访问wifi网络信息，wifi信息会用于进行网络定位-->
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"></uses-permission>
    <!-- 获取运营商信息，用于支持提供运营商信息相关的接口-->
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"></uses-permission>
    <!-- 这个权限用于获取wifi的获取权限，wifi信息会用来进行网络定位-->
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE"></uses-permission>
    <!-- 用于读取手机当前的状态-->
    <uses-permission android:name="android.permission.READ_PHONE_STATE"></uses-permission>
    <!-- 写入扩展存储，向扩展卡写入数据，用于写入离线定位数据-->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>
    <!-- 访问网络，网络定位需要上网-->
    <uses-permission android:name="android.permission.INTERNET" />
    <!-- SD卡读取权限，用户写入离线定位数据-->
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS"></uses-permission>


    <application android:allowBackup="true"
                 android:label="@string/app_name"
                 android:supportsRtl="true"
        >


        <activity android:name="consumer.fin.rskj.com.library.activitys.CreditLoanActivity"
                  android:screenOrientation="portrait"/>


        <!--绑定身份证页面-->
        <activity android:name="consumer.fin.rskj.com.library.activitys.FaceIDCardInfoUploadActivity"
                  android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="consumer.fin.rskj.com.library.activitys.FaceIDCardInfoUploadActivity"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>

        <!--身份证获取照片-->
        <activity android:name="consumer.fin.rskj.com.library.activitys.IDCardScanActivity"
                  android:screenOrientation="portrait"
                  android:configChanges="orientation|keyboardHidden"
                  android:theme="@android:style/Theme.Holo.NoActionBar.Fullscreen"/>

        <!--身份信息验证页面-->
        <activity android:name="consumer.fin.rskj.com.library.activitys.DealSelfInfoActivity"
                  android:screenOrientation="portrait">
        <intent-filter>
            <action android:name="consumer.fin.rskj.com.library.activitys.DealSelfInfoActivity"/>
            <category android:name="android.intent.category.DEFAULT"/>
        </intent-filter>
    </activity>

        <!--人脸识别起始页-->
        <activity android:name="consumer.fin.rskj.com.library.activitys.FaceStartActivity"
                  android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="consumer.fin.rskj.com.library.activitys.FaceStartActivity"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>

        <!--人脸识别检测页-->
        <activity android:name="consumer.fin.rskj.com.library.activitys.FaceActivity"
                  android:screenOrientation="portrait"/>

        <!--人脸识别结果页-->
        <activity android:name="consumer.fin.rskj.com.library.activitys.FaceResultActivity"
                  android:screenOrientation="portrait"/>

        <!--基本信息页面-->
        <activity android:name="consumer.fin.rskj.com.library.activitys.BasicInfoActivity"
                  android:screenOrientation="portrait">
        <intent-filter>
            <action android:name="consumer.fin.rskj.com.library.activitys.BasicInfoActivity"/>
            <category android:name="android.intent.category.DEFAULT"/>
        </intent-filter>
    </activity>


        <!--用户页面-->
        <activity android:name="consumer.fin.rskj.com.library.activitys.PersonCenterActivity"
                  android:screenOrientation="portrait"/>
        <!--银行卡列表-->
        <activity android:name="consumer.fin.rskj.com.library.activitys.BankCardListActivity"
                  android:screenOrientation="portrait"/>

        <!--银行卡信息-->
        <activity android:name="consumer.fin.rskj.com.library.activitys.CardDetailActivity"
                  android:screenOrientation="portrait"/>

        <!--账单列表-->
        <activity android:name="consumer.fin.rskj.com.library.activitys.MyBillListActivity"
                  android:screenOrientation="portrait"/>

        <!--借还款记录-->
        <activity android:name="consumer.fin.rskj.com.library.activitys.RecordListActivity"
                  android:screenOrientation="portrait"
                  android:windowSoftInputMode="adjustResize"/>

        <!--还款确认页面-->
        <activity android:name="consumer.fin.rskj.com.library.activitys.PaymentConfirmActivity"
                  android:screenOrientation="portrait"
                  android:windowSoftInputMode="adjustResize"/>

        <!--借款借据页面-->
        <activity android:name="consumer.fin.rskj.com.library.activitys.LoanCredentialsActivity"
                  android:screenOrientation="portrait"/>

        <!--选择银行卡页面-->
        <activity android:name="consumer.fin.rskj.com.library.activitys.ChooseBankActivity"
                  android:screenOrientation="portrait"/>

        <!--贷款申请页面-->
        <activity android:name="consumer.fin.rskj.com.library.activitys.ApplyLoanActivity"
                  android:screenOrientation="portrait"/>

        <!--设置支付密码-->
        <activity android:name="consumer.fin.rskj.com.library.activitys.SetPayPWDActivity"
                  android:screenOrientation="portrait"/>

        <activity android:name="consumer.fin.rskj.com.library.activitys.WebViewActivity"
                  android:screenOrientation="portrait"
            android:launchMode="singleTask" android:exported="true">
            <intent-filter>
                <action android:name="consumer.fin.rskj.com.library.activitys.html"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>

        </activity>

        <!--添加银行卡-->
        <activity android:name="consumer.fin.rskj.com.library.activitys.AddBankCardActivity"
                  android:screenOrientation="portrait"/>

        <!--马上还-->
        <activity android:name="consumer.fin.rskj.com.library.activitys.MaSHHActivity"
                  android:screenOrientation="portrait"/>

        <!--借款列表 -->
        <activity android:name="consumer.fin.rskj.com.library.activitys.LoanListActivity"
                  android:screenOrientation="portrait"/>

        <!--<activity android:name="consumer.fin.rskj.com.library.activitys.ByStadgeActivity"/>-->
        <activity android:name="consumer.fin.rskj.com.library.activitys.CurrentBillActivity"
                  android:screenOrientation="portrait"/>

        <activity android:name="consumer.fin.rskj.com.library.activitys.HtmlActivity2"
                  android:screenOrientation="portrait"/>
        <!--变更银行卡-->
        <activity android:name="consumer.fin.rskj.com.library.activitys.ChangeBankActivity"
                  android:screenOrientation="portrait"/>

        <activity android:name="consumer.fin.rskj.com.library.activitys.ReSetPayPWDActivity"
                  android:screenOrientation="portrait" />

        <!-- 白骑士键盘调用 -->
        <activity
            android:name="com.wknight.keyboard.WKnightKeyboardActivity"
            android:theme="@style/wKnightDialogStyleBottom" >
        </activity>
        <activity
            android:name="com.wknight.shield.FingerPrintDialog"
            android:theme="@android:style/Theme.Holo.Dialog" />

        <service
            android:name="com.wknight.keyboard.WKnightService"
            android:exported="false" />

    </application>

</manifest>
